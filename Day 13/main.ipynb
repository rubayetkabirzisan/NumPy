   "source": [
    "Copy vs Views Numpy Python Array"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a11e5ba9",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "var=np.array([1,2,3,4])\n",
    "co=var.copy()\n",
    "var[1]=40\n",
    "print(\"var : \",var)\n",
    "print()\n",
    "print(\"copy : \",co)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a69d8a48",
   "metadata": {},
   "outputs": [],
   "source": [
    "x=np.array([9,8,7,6,5])\n",
    "vi=x.view()\n",
    "x[1]=90\n",
    "print(\"x : \",x)\n",
    "print(\"view : \",vi)"
   ]
  }
 ],
